@using DojoSurveyVal.Models
@model Survey

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Profile</title>
    <link rel="stylesheet" href="~/css/display.css">
</head>
<body>
<div class="container">
    <div class="profile">
        <h1>Submitted Info</h1>
        @{
            <div class="name">
                <h3>Name:     @Model.Name</h3>
            </div>
            <div class="location">
                <h3>Dojo Location:     @Model.Location</h3>
            </div>
            <div class="favorite_language">
                <h3>Favorite Language:     @Model.Language</h3>
            </div>
            <div class="comment">
                <h3>Comment:     @Model.Comment</h3>
            </div>
            <button><a href="/">Go Back</a></button>
        }
    </div>
</div>
    
</body>
</html>